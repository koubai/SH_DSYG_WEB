<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="home" extends="struts-default, json-default" namespace="/home">
	
		<action name="goHomeAction" class="homeAction" method="goHomeAction">
			<result name="success">/page/home/home.jsp</result>
		</action>
		
		<action name="querySubItem" class="homeAction" method="querySubItem">
			<result type="json"></result>
		</action>
		
	</package>

</struts>